conf t

ip access-list extended CAST_VLAN10_IN
 permit ip 192.168.2.0 0.0.0.255 192.168.0.0 0.0.0.255
 deny   ip 192.168.2.0 0.0.0.255 192.168.1.0 0.0.0.255
 deny   ip 192.168.2.0 0.0.0.255 192.168.3.0 0.0.0.255
 permit ip 192.168.2.0 0.0.0.255 any

ip access-list extended CAST_VLAN20_IN
 permit ip 192.168.3.0 0.0.0.255 192.168.1.0 0.0.0.255
 deny   ip 192.168.3.0 0.0.0.255 192.168.0.0 0.0.0.255
 deny   ip 192.168.3.0 0.0.0.255 192.168.2.0 0.0.0.255
 permit ip 192.168.3.0 0.0.0.255 any

interface fa0/0.10
 ip access-group CAST_VLAN10_IN in

interface fa0/0.20
 ip access-group CAST_VLAN20_IN in

end
wr
